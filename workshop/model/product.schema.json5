{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "product_id": {
      "type": "string",
      "minLength": 1
    },
    "name": {
      "type": "string"
    },
    "quantity": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": [
    "product_id",
    "name",
    "quantity"
  ],
  "examples": [
    {
      "product_id": 15,
      "name": "apple",
      "quantity": 3
    }
  ]
}